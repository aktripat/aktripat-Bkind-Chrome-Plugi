{
  "manifest_version": 2,
  "name": "Social_Media_Extension",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "sm_icon.png"
  }
}
